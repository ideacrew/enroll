$("#audit-table-data").html("<%= escape_javascript(render "audit_data") %>");

$('.audit-with-arrow').on('click', function() {
  const auditId = this.getAttribute('data-attr-id');
  $("#audit-expansion-" + auditId).toggleClass('display-none')

  var icon = $(this).find('.glyphicon');

  // Toggle the class of the span element
  icon.toggleClass('glyphicon-chevron-down glyphicon-chevron-right');
//  $("#audit-" + auditId).toggleClass('glyphicon-chevron-right')
//  $("#audit-" + auditId).toggleClass('glyphicon-chevron-down')
});
