<div class="container-fluid">
  <div class="health">
    <div class="row inline-margin-top-2-percent">
      <div class="col-sm-2 text-right">
          % <%= l10n(".for_employee")%>:
      </div>
      <div class='col-sm-1'>
        <%= number_field_tag 'employee_slide_input', '', {  class: "slider_input hidden-param" } %>
      </div>
      <div class="col-sm-2">
        <input id="pct_employee" class='slider inline-width-200px' data-slider-id='pct_employeeSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="5" data-slider-value="50"/>
      </div>
      <div class="col-sm-1">
        <span id='employee_slide_label' class="slide-label pull-right">0%</span>
      </div>
      <div class="col-sm-2 text-right">
          % <%= l10n(".for_spouse")%>:
      </div>
      <div class='col-sm-1'>
        <%= number_field_tag 'spouse_input', '', {  class: "slider_input right_slider_input hidden-param" } %>
      </div>
      <div class="col-sm-2">
        <input id="pct_spouse" class='slider right-slider inline-width-200px' data-slider-id='pct_spouseSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="5" data-slider-value="0"/>
      </div>
      <div class="col-sm-1">
        <span id='spouse_label' class="slide-label pull-right">0%</span>
      </div>
    </div>
    <div class="row inline-margin-top-5-percent">
      <div class="col-sm-2 text-right">
          % <%= l10n(".for_domestic_partner")%>:
      </div>
      <div class='col-sm-1'>
        <%= number_field_tag 'domestic_input', '', {  class: "slider_input hidden-param" } %>
      </div>
      <div class="col-sm-2">
        <input id="pct_domestic_partner" class='slider inline-width-200px' data-slider-id='pct_domestic_partnerSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="5" data-slider-value="0"/>
      </div>
      <div class="col-sm-1">
        <span id='domestic_label' class="slide-label pull-right">0%</span>
      </div>
      <div class="col-sm-2 text-right">
          % <%= l10n(".for_child")%>:
      </div>
      <div class='col-sm-1'>
        <%= number_field_tag 'child_input', '', {  class: "slider_input right_slider_input hidden-param" } %>
      </div>
      <div class="col-sm-2">
        <input id="pct_child_under_26" class='slider right-slider inline-width-200px' data-slider-id='pct_child_under_26Slider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="5" data-slider-value="0"/>
      </div>
      <div class="col-sm-1">
        <span id='child_label' class="slide-label pull-right">0%</span>
      </div>
    </div>
  </div>
</div>
