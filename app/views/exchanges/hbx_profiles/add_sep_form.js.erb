    $('.admin_effective_on_kind_options #effective_on_date').val("");
    $('.init_effective_date input').val("");
    //$('.start_end_date').show();
  }
    $('.end_date input').val("");
    $('init_effective_date input').val("");

    <% if @bs4 %>
      showElement($('.init_event_datel'));
    <% else %>
