<% if @qualified_date %>
  $('#qle-date-chose').hide();
  $("#qle_reason").show();
  $("#qle_reason").html("<%= escape_javascript(render 'moving_fields') %>");
  $('#qle_reason .special_qle_reasons').removeClass('hidden');
  $('#qle-nav #qle_submit').prop('id', "qle_submit_reason");
  $("input:radio[name=reason]:first").click();
  var moving_notice = "<%= l10n('insured.qle_detail.special_reason.move', qle_start_date: @qle_date_calc, qle_date: @qle_date) %>";
  $("#date-change").html(moving_notice);
  $("input:radio[name=reason]:first").val(moving_notice);
<% else %>
  $.ajax({
      type: "GET",
      data:{date_val: $("#qle_date").val(), qle_id: $("#qle_id").val(), qle_reason_val: $("input:radio[name=reason]:checked").val(), bs4: $("#bs4").val()},
      url: "/insured/families/check_qle_date.js"
  });
<% end %>