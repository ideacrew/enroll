<% hbx_enrollments = @census_employee.past_enrollments %>
<% if hbx_enrollments.present? %>
  <h3>
    Past Enrollments
  </h3>
  <div class="panel panel-default module past-enrollments table-responsive">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th class="text-center">Termination Date</th>
          <th class="text-center">Plan Name</th>
          <th class="text-center">Market</th>
          <th class="text-center">Coverage</th>
          <th class="text-center">Premium</th>
          <th class="text-center">Effective Date</th>
          <th class="text-center">Covered</th>
          <th class="text-center">Status</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: 'shared/past_enrollment', collection: hbx_enrollments %>
      </tbody>
    </table>
  </div>
<% end %>