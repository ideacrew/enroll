<% qhps = local_assigns[:qhps] || @qhps%>
<% @visit_types.each do |visit_type| %>
  <tr>
    <th><%= visit_type %></th>
    <% qhps.each do |qhp|%>
      <% service_visit = qhp.qhp_service_visits.detect{|a| a.visit_type == visit_type} %>
      <% if service_visit.present? %>
        <td colspan="1" class="rb1"><%= service_visit.copay_in_network_tier_1 %></td>
        <td colspan="1" class="rb2"><%= service_visit.co_insurance_in_network_tier_1 %></td>
      <% else %>
        <td colspan="1" class="rb1"><%= "N/A" %></td>
        <td colspan="1" class="rb2"><%= "N/A" %></td>
      <% end %>
    <% end %>
  </tr>
<%end%>
