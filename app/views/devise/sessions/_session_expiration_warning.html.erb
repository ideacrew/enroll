<div class="modal" id="sessionExpirationWarningModal" tabindex="-1" role="dialog" role="document">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Warning</h4>
      </div>
      <div class="modal-body">
        <p>
          Your session will expire in
          <strong>
            <%= @time_left %>
          </strong>
          seconds. If youâ€™d like to continue with your application, select STAY LOGGED IN.
        </p>
        <ul class="application-warnings"></ul>
      </div>
      <div class="modal-footer">
        <%= link_to l10n(".logout"), destroy_user_session_path, method: "delete", class: "btn btn-default btn-small", tabindex: "0" %>
        <span tabindex="0", onkeydown="handleButtonKeyDown(event, 'stay-logged-in')", id="stay-logged-in" class="btn btn-primary  mtz btn-small" data-dismiss="modal"><%= l10n("faa.stay_logged_in") %></span>
      </div>
    </div>
  </div>
</div>
