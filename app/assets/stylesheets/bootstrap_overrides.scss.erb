@import "variables";

body {
  opacity: 0;
  display: none;
}

.navbar {
  margin-bottom: 0;
  background-color: $primary-button-text;
  border-bottom: solid 3px $primary-blue;
  .navbar-header {
    .navbar-brand {
      border-right: solid 1px #d7d7d7;
      min-height: 70px;
      img {
        max-height: 40px;
      }
    }
  }
  .navbar-collapse {
    .header-text {
      color: $primary-gray;
      font-size: 14px;
      text-transform: uppercase;
      font-weight: $primary-bold;
    }
    .navbar-nav:first-of-type {
      a.portal {
        text-align: center;
        vertical-align: middle;
        line-height: 70px;
        padding: 23px 15px;
        font-weight: 600;
        color: $primary-black;
        &:hover {
          background-color: $background-hover;
          text-decoration: none;
        }
      }
    }
    .navbar-right {
      li:last-of-type {
        text-align: right;
      }
      a.call-customer-service {
        span:first-of-type {}
        span:last-of-type {
          .fa {
            color: $primary-blue;
          }
          strong {
            line-height: 0;
            font-size: 16px;
            color: $primary-black;
          }
        }
      }
      .my-portal-links {
        min-height: 70px;
        padding: 15px 15px;
        display: block;
        border-left: solid 1px #d7d7d7;
        .users-name {
          color: $primary-black;
        }
      }
    }
  }
}

