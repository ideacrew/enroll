@import "variables";
/* Layout Specifics */

html {
  height: 100%
}

body {
    background-color: #f2f2f2;
    min-height: 100%;
    position: relative;
    padding-bottom: 70px;
}

h1, h2, h3, h4, h5, h6
  {
    font-weight: 300;
    line-height: 1.6em;
    }

  #new_person_wrapper p, .group-selection p {
    font-weight: 300;
  }

.navbar-default {
    background-color: #eee;
    background-image: none;
    box-shadow: none;
}

.navbar-white {
    background-color: #fff;
    border-bottom: 3px solid <%= EnrollRegistry[:qle_carousel].settings(:color).item %>;
    min-height: 73px;
}

#navbar .header-text {float: left;}
#navbar .header-text a {
  display: table-cell;
  height: 70px;
  vertical-align: middle;
  font-weight: 600;
  font-size: 18px;
  color: #5C5C5C;
}

.nav-info {
    font-size: 16px;
    font-weight: 300;
}

.not-sure-margin{
  margin-top: 14px;
  margin-left: -15px;
}

ul.brd-left li:first-child {
    border-left: 1px solid #dddddd;
}

.right-menu.navbar-right {
    margin: 0;
    padding-right: 15px;
}

a {
    color: <%= EnrollRegistry[:qle_carousel].settings(:color).item %>;
    transition: all 0.3s ease 0s;
}
.full {
  background-color: #f2f2f2;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}
.landing .container-fluid {
    padding: 0;
}
.container-fluid > .panel-default {
  border: none;
  box-shadow: none;
}

.focus_effect .disable-btn.disabled {
    background: none repeat scroll 0 0 #ccc;
}

.adderess-select-box + ul {
    border: 1px solid #bdbbbb;
    border-radius: 0;
    left: 0 !important;
    padding: 0;
    width: 100%;
}
.adderess-select-box + ul li {
    border-bottom: 1px solid #bdbbbb;
}
.adderess-select-box + ul li a {
    display: inline-block;
    height: 35px;
    margin: 0 5px;
    padding: 10px 0;
    text-align: center;
}
.adderess-select-box + ul li a:nth-child(2) {
    padding-top: 5px;
    width: 55%;
}
.adderess-select-box + ul li a:last-child {
    float: right;
}
.adderess-select-box + ul li:last-child {
    border-bottom: medium none;
}
.adderess-select-box + ul li:hover {
    background: none repeat scroll 0 0 #f5f5f5;
}

/* Flash Messages */

.alert-error {
    background-color: #f2dede;
    border-color: #eed3d7;
    color: #b94a48;
    text-align: left;
 }

.alert-alert {
    background-color: #f2dede;
    border-color: #eed3d7;
    color: #b94a48;
    text-align: left;
 }

.alert-success {
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #468847;
    text-align: left;
 }

.alert-notice {
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #468847;
    text-align: left;
 }

.alert-message {
    background-color: #ffd48a;
    border-color: #eed3d7;
    color: #333;
    text-align: left;
}

 .navbar-toggle .icon-bar {background-color: #000;}

.navbar-toggle {margin-top: 18px; margin-bottom: 18px;}

.social.aktif{
  background-color: <%= EnrollRegistry[:qle_carousel].settings(:color).item %>;
}
.social.border{
