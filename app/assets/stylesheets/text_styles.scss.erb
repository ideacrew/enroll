@import "variables";
.blue {
  color: #487ba2;
}
.darkblue {
  color: #3d5d6f;
}
.lg {
  color: #7b7b7b;
}
.dg {
  color: #414141;
}
.ttu {
  text-transform: uppercase;
}
.ttc {
  text-transform: capitalize;
}
.nine {
  font-size: 9px;
}
.twelve {
  font-size: 12px;
}
.fourteen {
  font-size: 14px;
}
.sixteen {
  font-size: 16px;
  font-weight: 300;
}
.twenty {
  font-size: 20px;
  font-weight: 300;
}
.wsn {
  white-space: nowrap;
}
.di {
  display: inline;
}
.tu {
  &:hover {
    text-decoration: underline;
  }
}
.alternate-header {
  color: $alternate-header-color;
}

