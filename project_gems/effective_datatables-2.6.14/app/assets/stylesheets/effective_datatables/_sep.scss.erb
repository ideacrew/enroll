#add_sep {

  p {
      margin: 0 0 10px;
      margin-top: 0px;
      margin-right: 0px;
      margin-bottom: 10px;
      margin-left: 0px;
  }
  p.label {
    width:100% !important;
  }
  textarea {
    width:100% !important;
    height:100% !important;
    border:none !important;
  }
  .btn-primary {
      color: #ffffff;
      background-color: #e72b2d;
      border-color: #b7191b;
      text-transform: uppercase;
      font-weight: 600;
      border-top: none;
      border-left: none;
      border-right: none;
      box-sizing: border-box;
      -moz-box-sizing: border-box;
      -webkit-box-sizing: border-box;
      text-shadow: none;
      -webkit-box-shadow: none;
      box-shadow: none;
      border-bottom-width: medium;
      width:15% !important;
      margin-left:auto !important;
      margin-right:auto !important;
  }
  .selectric {
      position: relative;
      background: #fff !important;
      height: 46px;
      width:230px !important;
      max-width:none !important;
  }
  .selectric-above .selectric-items {
      bottom: 100%;
      width: 200px !important;
  }
  .selectric-above .selectric-items {
      top: 50px;
      bottom: 100%;
  }
  .selectric .label {
      display: block;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
      line-height: 43px;
      height: 46px;
      max-width: 100%;
      font-weight: normal;
      font-size: 12px;
      text-align: left;
  }
  #effective_datatable_wrapper .selectric-wrapper {
      width: 200px !important;
  }
  table.dataTable thead .sorting_asc:after {
      /*content: "\e155";*/
      display:none !important;
  }
}
