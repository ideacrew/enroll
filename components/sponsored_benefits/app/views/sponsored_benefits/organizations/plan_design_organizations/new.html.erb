<div class="container">
  <div class="tab-content" id="myTabContent">
    <div class="col-md-12">
      <div class="row">
        <h3>Add Prospect Employer</h3>
      </div>
      <div class="row">
        <p><%= l10n('new_prospect_employer'%></p>
      </div>
      <br />
      <%= form_for @organization, as: :organization, url: {action: "create"} do |f| %>
        <%= hidden_field_tag :broker_agency_id, @broker_agency_profile.id.to_s %>
        <%= render 'shared/error_messages', :object => @organization %>
        <%= render 'form', f: f %>
      <% end %>
    </div>
  </div>
</div>
