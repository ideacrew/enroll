<div class="container">
  <%= render 'shared/engines/horizontal_status', step:1 %>
  <%= form_for @plan_design_proposal, url: {action: "update"}, method: :put do |f| %>
  <div class="row">
    <div class="col-sm-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">Quote for <%= @plan_design_proposal.plan_design_organization.legal_name %></h3>
        </div>
        <div class="panel-body">
          <%= render 'shared/error_messages', :object => @plan_design_proposal %>
          <%= render 'proposal_info', f: f %>
        </div>
      </div>
      <div class="pull-right">
        <%= f.submit "Save Quote", class: "btn btn-primary" %>
      </div>
    </div>
  </div>
  <% end %>
  <%= render 'employees' %>

