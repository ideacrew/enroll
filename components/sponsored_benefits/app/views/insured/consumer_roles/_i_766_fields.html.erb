<div id="immigration_i_766_fields_container">
  <% document = c.object.find_document("I-766 (Employment Authorization Card)") %>
  <%= c.fields_for 'vlp_documents', document do |v| %>
    <%= v.hidden_field :subject %>
    <%= v.hidden_field :id %>
    <%= render :partial => "insured/consumer_roles/docs_shared/alien_number", locals: {v: v} %>
    <%= render :partial => "insured/consumer_roles/docs_shared/card_number", locals: {v: v} %>
    <%= render :partial => "insured/consumer_roles/docs_shared/expiration_date", locals: {v: v, name: "I-766"} %>
  <% end %>
</div>
