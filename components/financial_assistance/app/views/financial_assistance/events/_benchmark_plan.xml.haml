%benchmark_product
  %id
    %id= financial_assistance_application.benchmark_product.hios_id
  %name= financial_assistance_application.benchmark_product.name
  %active_year= financial_assistance_application.benchmark_product.active_year
  %is_dental_only= financial_assistance_application.benchmark_product.is_dental_only?
  = render :partial=> 'financial_assistance/events/carrier', :object=> financial_assistance_application.benchmark_product.carrier_profile, as: :carrier
  %metal_level= "urn:openhbx:terms:v1:plan_metal_level##{financial_assistance_application.benchmark_product.metal_level}"
  %coverage_type= "urn:openhbx:terms:v1:qhp_benefit_coverage##{financial_assistance_application.benchmark_product.coverage_kind}"
  - if financial_assistance_application.benchmark_product.ehb
    %ehb_percent= financial_assistance_application.benchmark_product.ehb*100