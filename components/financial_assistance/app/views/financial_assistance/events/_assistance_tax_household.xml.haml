%assistance_tax_household
  %id
    %id= assistance_tax_household.hbx_assigned_id
  %primary_applicant_id
    %id= financial_assistance_application.primary_applicant.person_hbx_id
  %assistance_tax_household_members
    = render :partial => 'financial_assistance/events/assistance_tax_household_member', :collection => financial_assistance_application.active_applicants.in(is_applying_coverage: true), as: :assistance_tax_household_member, :locals=> {:financial_assistance_application => financial_assistance_application, :assistance_tax_household => assistance_tax_household}
