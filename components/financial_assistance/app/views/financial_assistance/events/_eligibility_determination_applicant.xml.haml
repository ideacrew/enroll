%eligibility_determination_applicant
  %individual
  = render :partial => "financial_assistance/events/individual", :object => eligibility_determination_applicant
  %is_primary_applicant= eligibility_determination_applicant.is_primary_applicant
  %is_consent_applicant= eligibility_determination_applicant.is_consent_applicant
  %is_coverage_applicant= eligibility_determination_applicant.is_coverage_applicant
  %is_without_assistance= eligibility_determination_applicant.is_without_assistance
  - if eligibility_determination_applicant.is_ia_eligible
    %is_insurance_assistance_eligible= eligibility_determination_applicant.is_ia_eligible?
  - if eligibility_determination_applicant.is_medicaid_chip_eligible
    %is_medicaid_chip_eligible= eligibility_determination_applicant.is_medicaid_chip_eligible?
  - if eligibility_determination_applicant.is_non_magi_medicaid_eligible
    %is_non_magi_medicaid_eligible= eligibility_determination_applicant.is_non_magi_medicaid_eligible
  - if eligibility_determination_applicant.is_totally_ineligible
    %is_totally_ineligible= eligibility_determination_applicant.is_totally_ineligible
  - unless eligibility_determination_applicant.is_disabled.blank? && eligibility_determination_applicant.is_tobacco_user.blank?
      %person_health
        - unless eligibility_determination_applicant.is_tobacco_user.blank?
          %is_tobacco_user= eligibility_determination_applicant.is_tobacco_user
        - unless eligibility_determination_applicant.is_disabled.blank?
          %is_disabled= eligibility_determination_applicant.is_disabled
