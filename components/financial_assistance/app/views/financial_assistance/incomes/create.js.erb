<% if @income.kind == 'wages_and_salaries' || @income.kind == 'self_employed_income' || @income.kind == 'net_self_employment'%>
  <%= render partial: 'replace', locals: { id: ".incomes-list .new-income-form" } %>
<% elsif @income.kind == 'unemployment_income' %>
  <%= render partial: 'replace', locals: { id: ".unemployment-incomes-list .new-unemployment-income-form" } %>
<% elsif @income.kind == 'american_indian_and_alaskan_native' %>
<%= render partial: 'replace', locals: { id: ".ai-an-incomes-list .new-ai-an-income-form" } %>
<% else %>
  <%= render partial: 'replace', locals: { id: ".other-incomes-list .new-other-income-form:not(.hidden)" } %>
<% end %>
