<div class="row hidden" id=<%="date_warning_message_#{form_object_id}"%>>
  <div class="col-md-12">
    <div class="alert alert-warning">
      <% current_year = TimeKeeper.date_of_record.year.to_s %>
      <% next_year = (TimeKeeper.date_of_record + 1.year).year.to_s %>
      <ul id="date_warnings_list">
        <li id=<%="start_date_warning_#{form_object_id}"%> class="hidden" data-cuke="start-date-warning">
          <%= l10n('faa.start_date_warning', current_year: current_year, next_year: next_year) %>
        </li>
        <li id=<%="end_date_warning_#{form_object_id}"%> class="hidden" data-cuke="end-date-warning">
          <%= l10n('faa.end_date_warning') %>
        </li>
      </ul>
    </div>
  </div>
</div>
