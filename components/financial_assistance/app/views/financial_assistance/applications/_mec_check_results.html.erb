<div class="er-medicaid mt-2">
  <div class="border-bottom-1px"></div>
    <div class="er-success bg-success">
        <div class="text-success pl-1 pr-1">
            <p>                   
              <% 
                medicaid_short_url = l10n("faa.mc_success_link")
                medicaid_agency_url = EnrollRegistry[:enroll_app].setting(:medicaid_agency_url).item
                medicaid_agency_phone = EnrollRegistry[:enroll_app].setting(:medicaid_agency_phone).item
              %>
              <b><%= l10n("faa.mc_success_existing", medicaid_agency_phone: link_to(medicaid_agency_phone, "tel:#{medicaid_agency_phone}", target: '_blank')) %></b>
              <%= l10n("faa.mc_success_enroll", medicaid_agency_url: link_to(medicaid_short_url, medicaid_agency_url, target: '_blank')) %>
              <%= l10n("faa.mc_success_phone") %>
            </p>
            <p>
              <b><%= l10n("faa.mc_continue_bold") %></b>
              <%= l10n("faa.mc_continue_application") %>
            </p>
        </div>
    </div>
</div>