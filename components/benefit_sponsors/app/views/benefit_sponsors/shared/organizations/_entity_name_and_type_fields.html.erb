<div>
  <div class="col-md-8 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :legal_name, class: "floatlabel form-control interaction-field-control-organization-legal-name typeahead_turned_off", required: true, placeholder: l10n("legal_name_caps") %>
  </div>
  <div class="col-md-4 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :dba, class: "floatlabel form-control interaction-field-control-organization-dba", placeholder: l10n("dba_caps") %>
  </div>
  <%= f.hidden_field :fein, value: f&.object&.fein %>
</div>
